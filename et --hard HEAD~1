    #include <thrust/partition.h> in generic/partition.inl
    #include <thrust/system/omp/detail/algorithm.h> at the end of thrust/system/omp/memory.h
    Introduce cpp/detail/malloc_and_free.h and cuda/detail/malloc_and_free.h
